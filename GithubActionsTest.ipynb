   "source": [
    "from datetime import datetime\n",
    "now = datetime.now()\n",
    "message = f\"The current time is {now.strftime('%H:%M:%S')} in New York City on {now.strftime('%m/%d/%Y')}.\"\n",
    "with open(now.strftime(\"%m-%d-%Y_%H-%M-%S\") + '.txt', 'w') as f:\n",
    "    f.write(message)"
   ]
